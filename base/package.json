{
  "name": "protomaps-themes-base",
  "version": "1.0.0",
  "description": "Protomaps basemap themes for MapLibre GL JS",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "scripts": {
    "build-iife": "esbuild src/index.js --outfile=dist/index.js --target=es6 --global-name=protomaps_themes_base --bundle --format=iife",
    "build-esm": "esbuild src/index.js --outfile=dist/index.mjs --target=es6 --bundle --format=esm",
    "build-cjs": "esbuild src/index.js --outfile=dist/index.cjs --target=es6 --bundle --format=cjs",
    "build": "npm run build-iife && npm run build-esm && npm run build-cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/protomaps/protomaps-themes.git"
  },
  "author": "Brandon Liu",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/protomaps/protomaps-themes#readme",
  "devDependencies": {
    "esbuild": "^0.12.8"
  }
}
